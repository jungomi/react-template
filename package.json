{
  "name": "react-template",
  "version": "0.1.0",
  "description": "Template for React",
  "main": "src/main.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --config build/webpack.dev.config.js",
    "build": "webpack --progress --config build/webpack.prod.config.js",
    "clean": "rm -rf dist coverage",
    "lint": "eslint .",
    "test": "ava --verbose",
    "watch:test": "npm run test -- --watch",
    "coverage": "nyc npm run test"
  },
  "ava": {
    "require": [
      "./test/helpers/setup.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "author": "Michael Jungo",
  "license": "MIT",
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.4.5",
    "json-loader": "^0.5.4",
    "nyc": "^8.1.0",
    "postcss-loader": "^0.11.1",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-server": "^2.1.0-beta.1",
    "webpack-merge": "^0.14.1"
  }
}
