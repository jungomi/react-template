{
  "name": "react-template",
  "version": "0.1.0",
  "description": "Template for React",
  "main": "src/main.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --config build/webpack.dev.config.js",
    "build": "webpack --progress --config build/webpack.prod.config.js",
    "clean": "rm -rf dist coverage",
    "lint": "eslint .",
    "test": "ava --verbose",
    "watch:test": "npm run test -- --watch",
    "coverage": "nyc npm run test"
  },
  "ava": {
    "require": [
      "./test/helpers/setup.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "author": "Michael Jungo",
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "ava": "^0.17.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.0.2",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "nyc": "^10.0.0",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "v3.0.0-beta.6",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0-rc.2",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "^2.0.0"
  }
}
